{
    "name": "aktaa-co/unit-conversions",
    "description": "Perform unit conversion in PHP",
    "keywords": [
        "Aktaa",
        "unit-conversions"
    ],
    "homepage": "https://github.com/mohammedaktaa/unit-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Aktaa",
            "email": "mohammedaktaa1995@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "AktaaCo\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AktaaCo\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor\\bin\\psalm",
        "test": "vendor\\bin\\phpunit",
        "test-coverage": "vendor\\bin\\phpunit --coverage-html coverage",
        "format": "vendor\\bin\\php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
